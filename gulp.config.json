{
  "client":"./html/",
  "html":"./html/**/*.html", 
  "htmltemplates": "./html/partials/**/*.html",
  "vendorjs": [
    "./html/lib/ionic/js/ionic.bundle.js",
    "./html/lib/moment/moment.js",
    "./html/lib/moment/locale/ar.js",
    "./html/lib/angular-translate/angular-translate.js",
    "./html/lib/angular-translate-loader-static-files/angular-translate-loader-static-files.js"
  ],
  "vendorcss": [
    "./html/lib/ionic/css/ionic.css"
  ],
  "css": [
    "./html/css/*.css"
  ],
  "js": [
    "./html/js/app.js",
    "./html/js/**/*.js",
    "./html/partials/**/*.js"
  ],
  "nodejs": [
    "./server/**/*.js"
  ],
  "fonts": [
    "./html/lib/ionic/fonts/**/*.*"
  ],
  "images": [
    "./html/images/**/*"
  ],
  "data":[
    "./html/data/**/*.json"
  ],
  "build": "./www/",
  "report":"./report"
}